<div id="content">
  <div class="container">
    <header class=" header23">
      <div class="text-box">
        <h1 class="qwer" id="ro2">Add Profile</h1>
      </div>
    </header>
    <div class="loginbox">
      <div class="addbookcard logincard">
    <form action="/addcon" method="post">
        <div class="details">
      <label for="name">Name</label><br>
        <input required type="text" name="name" value=""><br><br>
      </div>
  <div class="details">
      <label for="email">Email</label><br>
          <input required type="text" name="email" value=""><br><br>
        </div>
    <div class="details">
      <label for="gender">Gender</label>
      <select name="gender" class="dropdown">
        <option value="Male">Male</option>
        <option value="Female">Female</option>
        <option value="others">Others</option>
      </select><br>
      </div>
      <div class="details">
      <label for="year">Year</label>
